{
  "name": "nem-rest",
  "version": "0.0.6",
  "description": "Collaboration",
  "main": "src/server.js",
  "scripts": {
    "server": "nodemon ./src/server.js",
    "client": "npm start --prefix client",
    "start": "node /src/server.js",
    "test:jest": "jest --watchAll",
    "prepare": "husky install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:watch": "npm-run-all --parallel server client"
  },
  "keywords": [
    "express",
    "mongodb-driver",
    "nodejs",
    "jest",
    "API",
    "prettier",
    "jsonwebtoken",
    "bcrypt",
    "docker-compose",
    "docker"
  ],
  "author": "Bartlomiej Figatowski",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "grant": "^5.4.16",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.0.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-github2": "^0.1.12",
    "redis": "^3.1.2",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "jest": "^26.6.0",
    "lint-staged": "^11.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
